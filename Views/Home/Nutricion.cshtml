    ViewData["Title"] = "";
}

@section Styles {
    <link href="~/css/bienestar.css" rel="stylesheet" />
}


<h1>@ViewData["Title"]</h1>

    <style>
    
    .btn-warning {

        border-radius: 40px;
 
}
    
    </style>


    <div class="BlogHandler"> 
